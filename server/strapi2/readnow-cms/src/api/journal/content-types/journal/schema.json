{
  "kind": "collectionType",
  "collectionName": "journals",
  "info": {
    "singularName": "journal",
    "pluralName": "journals",
    "displayName": "Journals",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "journalTitle": {
      "type": "string"
    },
    "journalDescription": {
      "type": "text"
    },
    "journalPublishingDate": {
      "type": "date"
    },
    "journalEditorEmail": {
      "type": "email"
    },
    "journalTags": {
      "type": "json"
    },
    "journalCoverImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "chapters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chapter.chapter"
    },
    "isStandalone": {
      "type": "boolean"
    },
    "journalArticle": {
      "type": "blocks"
    },
    "publisherId": {
      "type": "string"
    },
    "journalComments": {
      "type": "json"
    },
    "journalLikes": {
      "type": "json"
    }
  }
}
